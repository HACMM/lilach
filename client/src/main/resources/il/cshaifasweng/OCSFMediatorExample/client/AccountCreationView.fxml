<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.AccountCreationController">
   <children>
      <Label layoutX="263.0" layoutY="60.0" prefHeight="27.0" prefWidth="73.0" text="Sign Up!" textFill="#911d69">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="nameField" layoutX="190.0" layoutY="128.0" prefHeight="30.0" prefWidth="220.0" promptText="Full Name" />
      <TextField fx:id="emailField" layoutX="190.0" layoutY="178.0" prefHeight="30.0" prefWidth="220.0" promptText="Email" />
      <TextField fx:id="usernameField" layoutX="190.0" layoutY="226.0" prefHeight="30.0" prefWidth="220.0" promptText="User Name" />
      <TextField fx:id="passwordField" layoutX="190.0" layoutY="273.0" prefHeight="30.0" prefWidth="220.0" promptText="Password" />
      <Button fx:id="signupBtn" layoutX="259.0" layoutY="333.0" mnemonicParsing="false" onAction="#onSignUpClicked" text="Sign me Up!" />
   </children>
</AnchorPane>

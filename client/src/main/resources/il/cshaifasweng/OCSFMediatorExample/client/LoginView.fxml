<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.Cursor?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="il.cshaifasweng.OCSFMediatorExample.client.LoginController">
   <children>
      <PasswordField fx:id="passwordField" layoutX="164.0" layoutY="182.0" prefHeight="35.0" prefWidth="242.0" promptText="Password" />
      <TextField fx:id="usernameField" layoutX="164.0" layoutY="119.0" prefHeight="35.0" prefWidth="242.0" promptText="User Name" />
      <Button fx:id="loginBtn" layoutX="209.0" layoutY="248.0" mnemonicParsing="false" onAction="#onLoginClicked" prefHeight="25.0" prefWidth="151.0" text="Log in" />
      <Label fx:id="SignUp" layoutX="184.0" layoutY="300.0" onMouseClicked="#OnSignUpClicked" prefHeight="17.0" prefWidth="201.0" text="Don't have an account? Sign up here!" textFill="#125ed0" underline="true">
         <cursor>
            <Cursor fx:constant="HAND" />
         </cursor>
      </Label>
   </children>
</AnchorPane>
